class Solution {
    public int repeatedStringMatch(String A, String B) {
        int res = 1;
        String temp = new String(A);
        while (!A.contains(B)) {
            A = A + temp;
            res++;
            if (A.length() > 10000) return -1;
        }
        return res;
    }
}


//------------------------------------------------------
class Solution {
    public int repeatedStringMatch(String A, String B) {
        int res = 1;
        String temp = new String(A);
        while (!A.contains(B)) {
            A = A + temp;
            res++;
            if (A.length() > B.length()) {
                if (A.contains(B)) return res;
                A = A + temp;
                res++;
                if (A.contains(B)) return res;
                return -1;
            }
        }
        return res;
    }
}
